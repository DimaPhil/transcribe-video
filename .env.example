OPENAI_API_KEY=
TELEGRAM_BOT_TOKEN=
ANTHROPIC_API_KEY=

# Optional: For Telegram Bot API local server (large file support)
# Get these from https://my.telegram.org/apps
TELEGRAM_API_ID=
TELEGRAM_API_HASH=

# Optional: Configure Telegram Bot API server paths
# These have sensible defaults and usually don't need to be changed
# TELEGRAM_API_DATA_DIR=/var/lib/telegram-bot-api  # Path inside telegram-bot-api container
# TELEGRAM_API_MOUNT_PATH=/telegram-bot-api-files  # Mount path in telegram-bot container

# Optional: Change web interface port (default: 8082)
# WEB_PORT=8082